<interfaces xmlns="..." xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="...">

    <types path="./types.xml" />

    <interface id="SkillInterface">
        <function id="tick">
            <protocol>
                <ROS2data>
                    <interface type="sync-service" />
                </ROS2data>
            </protocol>
            <returnValue>
                <dataType>enum</dataType>
                <enum refid="TickResponse" />
            </returnValue>
        </function>
        <function id="halt">
            <protocol>
                <ROS2data>
                    <interface type="sync-service" />
                </ROS2data>
            </protocol>
        </function>
    </interface>

    <interface id="template_interfaces">
        <function id="Function1">
            <protocol>
                <ROS2data>
                    <interface type="sync-service" />
                </ROS2data>
            </protocol>
        </function>
        <function id="Function2">
            <requestValue>
                <dataType>std_msgs::msg::Bool</dataType>
                <dataField>is_active</dataField>
            </requestValue>
            <returnValue>
                <dataType>std_msgs::msg::Bool</dataType>
                <dataField type="bool" name="is_active"></dataField>
            </returnValue>
        </function>
    </interface>

    <interface id="template_interfaces2">
        <function id="Function3">
            <protocol>
                <ROS2data>
                    <interface type="sync-service" />
                </ROS2data>
            </protocol>
        </function>
        <function id="Function4">
            <protocol>
                <ROS2data>
                    <interface type="topic" />
                </ROS2data>
            </protocol>
            <returnValue>
                <dataType>std_msgs::msg::Bool</dataType>
                <dataField>is_active</dataField>
            </returnValue>
        </function>
    </interface>
</interfaces>